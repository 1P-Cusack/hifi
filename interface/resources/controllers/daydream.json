{
    "name": "Daydream to Standard",
    "channels": [
        { "from": "Daydream.RY", "when": "Daydream.RSY", "filters": ["invert"], "to": "Standard.RY", "debug": true },
        { "from": "Daydream.RX", "when": "Daydream.RSX", "to": "Standard.RX", "debug": true },

        { "from": "Daydream.RTClick", "to": "Standard.RTClick", "peek": true, "debug": true},
        { "from": "Daydream.RS", "to": "Standard.RS" },
        { "from": "Daydream.RSTouch", "to": "Standard.RSTouch" },
		{ "from": "Daydream.RSCenter", "to": "Standard.RightPrimaryThumb" },

        { "from": "Daydream.RightHand", "to": "Standard.RightHand", "when": [ "Application.InHMD" ]  }    
    ]
}